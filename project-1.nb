Cell[BoxData[
 RowBox[{
  RowBox[{"getIndex", "[", "x_", "]"}], ":=", " ", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", "indexList", "}"}], ",", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"indexList", " ", "=", " ", 
      RowBox[{"Position", "[", 
       RowBox[{"black", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"If", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Length", "[", "indexList", "]"}], " ", "\[NotEqual]", " ", 
        "0"}], ",", " ", 
       RowBox[{"indexList", "[", 
        RowBox[{"[", 
         RowBox[{"1", ",", "1"}], "]"}], "]"}], ",", " ", 
       "\[IndentingNewLine]", 
       RowBox[{
        RowBox[{"indexList", " ", "=", " ", 
         RowBox[{"Position", "[", 
          RowBox[{"red", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"Length", "[", "indexList", "]"}], " ", "\[NotEqual]", " ",
            "0"}], ",", " ", 
          RowBox[{"indexList", "[", 
           RowBox[{"[", 
            RowBox[{"1", ",", "1"}], "]"}], "]"}], ",", "\[IndentingNewLine]", 
          RowBox[{
           RowBox[{"indexList", " ", "=", " ", 
            RowBox[{"Position", "[", 
             RowBox[{"green", ",", "x"}], "]"}]}], ";", "\[IndentingNewLine]", 
           RowBox[{"If", "[", 
            RowBox[{
             RowBox[{
              RowBox[{"Length", "[", "indexList", "]"}], " ", "\[NotEqual]", 
              " ", "0"}], ",", " ", 
             RowBox[{"indexList", "[", 
              RowBox[{"[", 
               RowBox[{"1", ",", "1"}], "]"}], "]"}], ",", 
             "\[IndentingNewLine]", 
             RowBox[{
              RowBox[{"indexList", " ", "=", " ", 
               RowBox[{"Position", "[", 
                RowBox[{"blue", ",", "x"}], "]"}]}], ";", 
              "\[IndentingNewLine]", 
              RowBox[{"If", "[", 
               RowBox[{
                RowBox[{
                 RowBox[{"Length", "[", "indexList", "]"}], " ", 
                 "\[NotEqual]", " ", "0"}], ",", " ", 
                RowBox[{"indexList", "[", 
                 RowBox[{"[", 
                  RowBox[{"1", ",", "1"}], "]"}], "]"}], ",", " ", 
                RowBox[{"-", "1"}]}], "]"}]}]}], "]"}]}]}], "]"}]}]}], 
