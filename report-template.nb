Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"straightQ", "[", "hand_", "]"}], ":=", 
   RowBox[{
    RowBox[{
     RowBox[{"Sort", "[", 
      RowBox[{"Mod", "[", 
       RowBox[{"hand", ",", "100"}], "]"}], "]"}], "-", 
     RowBox[{"Mod", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"Sort", "[", "hand", "]"}], "[", 
        RowBox[{"[", "1", "]"}], "]"}], ",", "100"}], "]"}]}], "\[Equal]", 
    RowBox[{"{", 
     RowBox[{"0", ",", "1", ",", "2", ",", "3", ",", "4"}], "}"}]}]}], 
  ";"}], "\[IndentingNewLine]", 
