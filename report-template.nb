     RowBox[{"Sort", "[", "hand", "]"}], "-", 
     RowBox[{
      RowBox[{"Sort", "[", "hand", "]"}], "\[LeftDoubleBracket]", "1", 
      "\[RightDoubleBracket]"}]}], "\[Equal]", 
    RowBox[{"{", 
     RowBox[{"0", ",", "1", ",", "2", ",", "3", ",", "4"}], "}"}]}]}], 
